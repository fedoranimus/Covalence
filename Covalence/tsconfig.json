{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "lib": [ "es2015", "dom" ],
    "types": [ "node" ],
    "baseUrl": "./ClientApp/app/",
    "paths": {
      "services": ["services"],
      "infrastructure": ["infrastructure"]
    }
  },
  "exclude": [ "bin", "node_modules" ],
  "atom": { "rewriteTsconfig": false }
}
